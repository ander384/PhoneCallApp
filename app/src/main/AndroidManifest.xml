<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.exodus.ander.phoneapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_no5"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_no5"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.exodus.ander.phoneapp.DialScreenActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.exodus.ander.phoneapp.CallScreenActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>